<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.uh.tech.cis3368.semesterproject.JobViewerController">
   <children>
      <Label layoutX="338.0" layoutY="14.0" text="Job Viewer">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Button fx:id="btnCreateProduct" layoutX="232.0" layoutY="451.0" mnemonicParsing="false" onAction="#handleCreateProduct" text="Create Product" />
      <Button fx:id="btnDone" layoutX="470.0" layoutY="451.0" mnemonicParsing="false" onAction="#handleDone" text="Done" />
      <TableView fx:id="jobTableView" layoutX="16.0" layoutY="60.0" prefHeight="371.0" prefWidth="766.0">
        <columns>
          <TableColumn fx:id="colJobName" prefWidth="263.20001220703125" text="Name" />
          <TableColumn fx:id="colJobDescription" prefWidth="317.5999755859375" text="Description" />
            <TableColumn fx:id="colJobStage" prefWidth="184.79998779296875" text="Stage" />
        </columns>
      </TableView>
      <Button fx:id="btnMoveToNextStage" layoutX="338.0" layoutY="451.0" mnemonicParsing="false" onAction="#handleMoveStage" text="Move to Next Stage" />
   </children>
</AnchorPane>
